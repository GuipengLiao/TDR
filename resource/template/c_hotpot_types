#ifndef _H_<% $file_tag %>
#define _H_<% $file_tag %>
<%
	$DefinitionList
	[
		$*
		{
			$comment
			{
%>
//<% $text %>
<%
			}


			$const
			{
%>
#ifndef _DEF_<% $name %>
#define <% $name %> <% $value %>
#endif//_DEF_<% $name %>
<%
			}

			$enum
			{
%>
typedef enum _<% $name %>
{
<%
				$list
				[
					$*
					{
%>
	<% $name %> = <% $value %> ,<%	}
				]
%>
}<% $name %>
<%
			}

			$struct
			{
%>
typedef struct _<% $name %>
{
<%				$list
				[
					$*
					{
%>
	<%$Type{$SimpleType{$type} $ObjectType{$type} $ContainerType{$vector{$Arguments[$0{$name}]}}}%> <%$name%>;<%
					}
				]
%>
}<% $name %>
<%
			}

			$union
			{
%>
typedef union _<% $name %>
{
<%				$list
				[
					$*
					{
%>
	<%$Type{$SimpleType{$type} $ObjectType{$type} $ContainerType{$vector{$Arguments[$0{$name}]}}}%> <% $name%>;<%
					}
				]
%>
}<% $name %>
<%
			}
		
			$typedef
			{
%>
typedef <%$Type{$SimpleType{$type} $ObjectType{$type} $ContainerType{$vector{$Arguments[$0{$name}]}}}%> <%$new_type%>;
<%
			}
		}
	]
%>
#endif//_H_<% $file_tag %>
